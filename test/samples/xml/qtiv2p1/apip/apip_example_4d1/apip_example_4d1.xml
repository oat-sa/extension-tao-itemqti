     <itemBody>
         <p id="read_one">Consider this expression.</p>
         <p>
             <math xmlns="http://www.w3.org/1998/Math/MathML" id="read_two">
                 <mrow>
                     <mi> k </mi>
                     <mo> ( </mo>
                     <mrow>
                         <mrow>
                             <mi> m </mi> <mo> * </mo> <mi> k </mi>
                         </mrow>
                         <mo> - </mo>
                         <msup>
                             <mi> k </mi> <mn> 2 </mn>
                         </msup>
                     </mrow>
                     <mo> ) </mo>
                 </mrow>
             </math>
         </p>
         <choiceInteraction responseIdentifier="RESPONSE" shuffle="false" maxChoices="1" id="choicereduction">
             <prompt id="prompt">What is the value of this expression when k=-1 and m=2 ?</prompt>
             <simpleChoice identifier="minus-two" fixed="true">
                 <p id="p4">-2</p>
             </simpleChoice>
             <simpleChoice identifier="zero" fixed="true">
                 <p id="p5">0</p>
             </simpleChoice>
             <simpleChoice identifier="one" fixed="true">
                 <p id="p6">1</p>
             </simpleChoice>
             <simpleChoice identifier="three" fixed="true">
                 <p id="p7">3</p>
             </simpleChoice>
         </choiceInteraction>
     </itemBody>
    
