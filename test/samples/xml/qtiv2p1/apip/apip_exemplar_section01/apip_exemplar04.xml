    <responseDeclaration identifier="RESPONSE" baseType="identifier" cardinality="single">
        <correctResponse>
            <value>fourthSentence</value>
        </correctResponse>
    </responseDeclaration>
    <outcomeDeclaration identifier="SCORE" cardinality="single" baseType="float">
        <defaultValue>
            <value>0</value>
        </defaultValue>
    </outcomeDeclaration>
    <outcomeDeclaration identifier="MAXSCORE" cardinality="single" baseType="float">
        <defaultValue>
            <value>1</value>
        </defaultValue>
    </outcomeDeclaration>
    <itemBody id="content4">

        <orderInteraction id="content1" maxChoices="5" minChoices="0" shuffle="false"
            orientation="vertical" responseIdentifier="RESPONSE">
            <prompt id="content2"> Put the following sentences in the order in which they happened
                in the story. </prompt>
            <simpleChoice showHide="show" identifier="firstSentence" id="content5" fixed="true">
                Anina saw the crocodile. </simpleChoice>
            <simpleChoice showHide="show" identifier="secondSentence" id="content3" fixed="true">
                The crocodile ate two flamingos. </simpleChoice>
            <simpleChoice showHide="show" identifier="thirdSentence" id="content6" fixed="true">
                Anina tried to explain the broken door to her parents. </simpleChoice>
            <simpleChoice showHide="show" identifier="fourthSentence" id="content8" fixed="true">
                Anina started to walk to the bathroom. </simpleChoice>
            <simpleChoice showHide="show" identifier="fifthSentence" id="content7" fixed="true">
                Anina ran to the bedroom and slammed the door. </simpleChoice>
        </orderInteraction>
    </itemBody>
    <responseProcessing>
        <responseCondition>
            <responseIf>
                <match>
                    <variable identifier="RESPONSE"/>
                    <correct identifier="RESPONSE"/>
                </match>
                <setOutcomeValue identifier="SCORE">
                    <variable identifier="MAXSCORE"/>
                </setOutcomeValue>
            </responseIf>
        </responseCondition>
    </responseProcessing>
